<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.card.mapper.CardMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.card.entity.Card">        <id column="ID" property="id" />        <result column="NAME" property="name" />        <result column="NO" property="no" />        <result column="AMOUNT" property="amount" />        <result column="GIVE_AMOUNT" property="giveAmount" />        <result column="GOOD_DISCOUNT" property="goodDiscount" />        <result column="PROJECT_DISCOUNT" property="projectDiscount" />        <result column="CREATE_ID" property="createId" />        <result column="CREATE_NAME" property="createName" />        <result column="DEPT_ID" property="deptId" />        <result column="CREATE_TIME" property="createTime" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        ID, NAME,NO, AMOUNT,GIVE_AMOUNT,GOOD_DISCOUNT, PROJECT_DISCOUNT, CREATE_ID, CREATE_NAME, DEPT_ID, CREATE_TIME    </sql>    <select id="customPageList" resultType="com.sweet.modular.card.entity.Card"            parameterType="com.sweet.modular.card.entity.Card">        select        <include refid="Base_Column_List"/>        from business_card where 1 = 1       <if test="param.id != null and param.id != ''">        and ID like concat('%',#{param.id},'%')       </if>       <if test="param.name != null and param.name != ''">        and NAME like concat('%',#{param.name},'%')       </if>    </select></mapper>