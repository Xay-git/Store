<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.stock.mapper.StockMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.stock.entity.Stock">        <id column="ID" property="id" />        <result column="NAME" property="name" />        <result column="STOCK_NO" property="stockNo" />        <result column="DEPT_ID" property="deptId" />        <result column="DEPT_NAME" property="deptName" />        <result column="CREATE_ID" property="createId" />        <result column="CREATE_NAME" property="createName" />        <result column="CREATE_TIME" property="createTime" />        <result column="REMARK" property="remark" />        <result column="CHECK_ID" property="checkId" />        <result column="CHECK_NAME" property="checkName" />        <result column="CHECK_TIME" property="checkTime" />        <result column="STOCK_STATUS" property="stockStatus" />        <result column="STOCK_TYPE" property="stockType" />        <result column="STOCK_MONEY" property="stockMoney" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        ID, NAME, STOCK_NO, DEPT_ID, DEPT_NAME, CREATE_ID, CREATE_TIME,CREATE_NAME, REMARK, CHECK_ID, CHECK_NAME, CHECK_TIME, STOCK_STATUS, STOCK_TYPE, STOCK_MONEY    </sql>    <select id="customPageList" resultType="com.sweet.modular.stock.entity.Stock"            parameterType="com.sweet.modular.stock.entity.Stock">        select        <include refid="Base_Column_List"/>        from business_stock where 1 = 1       <if test="param.name != null and param.name != ''">        and NAME like concat('%',#{param.name},'%')       </if>       <if test="param.deptName != null and param.deptName != ''">        and DEPT_NAME like concat('%',#{param.deptName},'%')       </if>       <if test="param.stockType != null and param.stockType != ''">        and STOCK_TYPE like concat('%',#{param.stockType},'%')       </if>       order by create_time desc    </select></mapper>