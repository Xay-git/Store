<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.stockDetail.mapper.StockDetailMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.stockDetail.entity.StockDetail">        <id column="ID" property="id" />        <result column="PRODUCT_ID" property="productId" />        <result column="PRODUCT_NAME" property="productName" />        <result column="COUNT" property="count" />        <result column="CREATE_TIME" property="createTime" />        <result column="STOCK_ID" property="stockId" />        <result column="DEPT_ID" property="deptID" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        ID, PRODUCT_ID, PRODUCT_NAME,DEPT_ID, COUNT, CREATE_TIME, STOCK_ID    </sql>    <select id="customPageList" resultType="com.sweet.modular.stockDetail.entity.StockDetail"            parameterType="com.sweet.modular.stockDetail.entity.StockDetail">        select        <include refid="Base_Column_List"/>        from business_stock_detail where 1 = 1       <if test="param.stockId != null and param.stockId != ''">        and STOCK_ID like concat('%',#{param.stockId},'%')       </if>    </select></mapper>