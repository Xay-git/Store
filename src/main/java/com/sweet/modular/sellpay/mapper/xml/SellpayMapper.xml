<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.sellpay.mapper.SellpayMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.sellpay.entity.Sellpay">        <id column="ID" property="id" />        <result column="SELL_ID" property="sellId" />        <result column="PAY_NAME" property="payName" />        <result column="PAY_TYPE" property="payType" />        <result column="AMOUNT" property="amount" />        <result column="CREATE_TIME" property="createTime" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        ID, SELL_ID, PAY_NAME, PAY_TYPE, AMOUNT, CREATE_TIME    </sql>    <select id="customPageList" resultType="com.sweet.modular.sellpay.entity.Sellpay"            parameterType="com.sweet.modular.sellpay.entity.Sellpay">        select        <include refid="Base_Column_List"/>        from business_sellpay where 1 = 1       <if test="param.cellId != null and param.cellId != ''">        and CELL_ID like concat('%',#{param.cellId},'%')       </if>       order by create_time desc    </select></mapper>