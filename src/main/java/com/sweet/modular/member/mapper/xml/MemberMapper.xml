<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.member.mapper.MemberMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.member.entity.Member">        <id column="ID" property="id" />        <result column="NAME" property="name" />        <result column="BALANCE" property="balance" />        <result column="SEX" property="sex" />        <result column="BIRTHDAY" property="birthday" />        <result column="PHONE_NUMBER" property="phoneNumber" />        <result column="ADDRESS" property="address" />        <result column="COME_FROM" property="comeFrom" />        <result column="DEPT_ID" property="deptId" />        <result column="INVITER" property="inviter" />        <result column="MEMBER_STATUS" property="memberStatus" />        <result column="REMARK" property="remark" />        <result column="CREATE_TIME" property="createTime" />        <result column="AVATAR" property="avatar" />        <result column="INVITER_ID" property="inviterId" />        <result column="USER_ID" property="userId" />        <result column="SMS_ALERT" property="smsAlert" />        <result column="OPEN_ID" property="openId" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        ID, NAME, SEX, BIRTHDAY, PHONE_NUMBER, ADDRESS, COME_FROM, DEPT_ID, INVITER, MEMBER_STATUS, REMARK, CREATE_TIME, AVATAR, INVITER_ID, USER_ID, SMS_ALERT, OPEN_ID    </sql>    <select id="customPageList" resultType="com.sweet.modular.member.entity.Member"            parameterType="com.sweet.modular.member.entity.Member">        select        <include refid="Base_Column_List"/>        from business_member where 1 = 1       <if test="param.name != null and param.name != ''">        and (NAME like concat('%',#{param.name},'%')       </if>       <if test="param.phoneNumber != null and param.phoneNumber != ''">        or PHONE_NUMBER like concat('%',#{param.phoneNumber},'%'))       </if>       <if test="param.deptId != null and param.deptId != ''">        and DEPT_ID = #{param.deptId}       </if>       and MEMBER_STATUS = 1       order by create_time desc    </select></mapper>