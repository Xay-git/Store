<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.receipt.mapper.ReceiptMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.receipt.entity.Receipt">        <id column="ID" property="id" />        <result column="TRADE_NO" property="tradeNo" />        <result column="TRM_NO" property="trmNo" />        <result column="AMOUNT" property="amount" />        <result column="SELL_ID" property="sellId" />        <result column="CREATE_TIME" property="createTime" />        <result column="CREATE_ID" property="createId" />        <result column="CREATE_NAME" property="createName" />        <result column="DEPT_ID" property="deptId" />        <result column="COLL_STATUS" property="collStatus" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        ID, TRADE_NO, TRM_NO, AMOUNT, SELL_ID, CREATE_TIME, CREATE_ID, CREATE_NAME, DEPT_ID, COLL_STATUS    </sql>    <select id="customPageList" resultType="com.sweet.modular.receipt.entity.Receipt"            parameterType="com.sweet.modular.receipt.entity.Receipt">        select        <include refid="Base_Column_List"/>        from business_receipt where 1 = 1       <if test="param.id != null and param.id != ''">        and ID like concat('%',#{param.id},'%')       </if>    </select></mapper>