<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.selldetail.mapper.SelldetailMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.selldetail.entity.Selldetail">        <id column="ID" property="id" />        <id column="NAME" property="name" />        <result column="SELL_ID" property="sellId" />        <result column="PRODUCT_ID" property="productId" />        <result column="MEMBER_CARD_ID" property="memberCardId" />        <result column="CARD_ID" property="cardId" />        <result column="SELL_TYPE" property="sellType" />        <result column="TECH_ID" property="techId" />        <result column="TECH_NAME" property="techName" />        <result column="AMOUNT" property="amount" />        <result column="REAL_AMOUNT" property="realAmount" />        <result column="DISCOUNT" property="discount" />        <result column="PUSH_MONEY" property="pushMoney" />        <result column="CREATE_TIME" property="createTime" />        <result column="MEMBER_CARD_AMOUNT" property="memberCardAmount" />        <result column="GIVE_AMOUNT" property="giveAmount" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        ID, NAME,SELL_ID, PRODUCT_ID, MEMBER_CARD_ID, CARD_ID, SELL_TYPE, TECH_ID, TECH_NAME, AMOUNT,DISCOUNT, REAL_AMOUNT,PUSH_MONEY, CREATE_TIME,GIVE_AMOUNT,MEMBER_CARD_AMOUNT    </sql>    <select id="customPageList" resultType="com.sweet.modular.selldetail.entity.Selldetail"            parameterType="com.sweet.modular.selldetail.entity.Selldetail">        select        <include refid="Base_Column_List"/>        from business_selldetail where 1 = 1       <if test="param.sellId != null and param.sellId != ''">        and SELL_ID like concat('%',#{param.sellId},'%')       </if>       <if test="param.techId != null and param.techId != ''">        and TECH_ID like concat('%',#{param.techId},'%')       </if>    </select></mapper>