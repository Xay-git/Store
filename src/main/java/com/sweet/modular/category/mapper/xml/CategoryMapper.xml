<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.category.mapper.CategoryMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.category.entity.Category">        <id column="ID" property="id" />        <result column="NAME" property="name" />        <result column="CREATE_ID" property="createId" />        <result column="CREATE_NAME" property="createName" />        <result column="CREATE_TIME" property="createTime" />        <result column="CATEGORY_TYPE" property="categoryType" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        ID, NAME, CREATE_ID, CREATE_NAME, CREATE_TIME, CATEGORY_TYPE    </sql>    <select id="customPageList" resultType="com.sweet.modular.category.entity.Category"            parameterType="com.sweet.modular.category.entity.Category">        select        <include refid="Base_Column_List"/>        from business_category where 1 = 1       <if test="param.name != null and param.name != ''">        and NAME like concat('%',#{param.name},'%')       </if>       order by category_type asc    </select></mapper>