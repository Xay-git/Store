<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.selldetailTech.mapper.DetailTechMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.selldetailTech.entity.DetailTech">        <id column="ID" property="id"/>        <result column="SELL_DETAIL_ID" property="sellDetailId"/>        <result column="TECH_ID" property="techId"/>        <result column="TECH_NAME" property="techName"/>        <result column="PUSH_MONEY" property="pushMoney"/>    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        ID, SELL_DETAIL_ID, TECH_ID, TECH_NAME, PUSH_MONEY    </sql>    <select id="customPageList" resultType="com.sweet.modular.selldetailTech.entity.DetailTech"            parameterType="com.sweet.modular.selldetailTech.entity.DetailTech">        select        <include refid="Base_Column_List"/>        from business_detail_tech where 1 = 1        <if test="param.techName != null and param.techName != ''">            and TECH_NAME like concat('%',#{param.techName},'%')        </if>    </select>    <insert id="saveBySelectByUserId"            parameterType="com.sweet.modular.selldetailTech.entity.DetailTech">        INSERT INTO business_detail_tech        (SELL_DETAIL_ID, TECH_ID, TECH_NAME, PUSH_MONEY)        VALUES        (#{param.sellDetailId}, #{param.techId}, (SELECT REAL_NAME FROM t_user WHERE USER_ID=#{param.techId}), '1');    </insert></mapper>